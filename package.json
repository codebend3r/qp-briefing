{
  "name": "qp-briefing",
  "version": "0.5.2",
  "private": true,
  "author": "CJ Rivas <cj.rivas.dev@gmail.com>",
  "scripts": {
    "branches:stale": "DRY_RUN=false bash bin/clean-stale.sh",
    "branches:stale:dr": "DRY_RUN=true bash bin/clean-stale.sh",
    "docker:build": "docker build -t codebend3r/ubuntu-node18-bun-zsh . -f .devcontainer/Dockerfile",
    "docker:build2": "docker build --platform=\"linux/arm64\" -t codebend3r/ubuntu-node18-bun-zsh . -f .devcontainer/Dockerfile",
    "docker:push": "docker push codebend3r/ubuntu-node18-bun-zsh",
    "build": "run-s info build:app",
    "build:app": "next build",
    "build:production": "run-s prisma:generate build",
    "info": "run-s info:node info:npm info:pnpm info:prisma",
    "info:node": "node -v",
    "info:npm": "npm -v",
    "info:pnpm": "pnpm -v",
    "info:bun": "bun -v",
    "info:prisma": "prisma -v",
    "dev": "next dev",
    "diff-exit-code": "git diff --exit-code",
    "lint": "npx eslint -c eslint.config.mjs ./",
    "lint:fix": "npx eslint -c eslint.config.mjs ./ --fix ",
    "pre-push": "bash bin/pre-push.sh",
    "prettier": "prettier --w .",
    "prettier:check": "prettier --check .",
    "prisma:db:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "start": "next start",
    "system-check": "run-s prisma:generate prettier lint build",
    "test": "vitest --watch=false",
    "test:watch": "vitest --watch",
    "update-packages": "pnpm update -i --latest"
  },
  "dependencies": {
    "@auth/core": "0.28.2",
    "@emotion/react": "11.13.3",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.13.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/font": "14.2.7",
    "@portabletext/react": "3.1.0",
    "@prisma/client": "5.19.0",
    "@sanity/image-url": "1.0.2",
    "@sendgrid/client": "8.1.2",
    "@tanstack/eslint-plugin-query": "5.62.16",
    "@tanstack/react-query": "5.56.2",
    "@tanstack/react-query-devtools": "5.56.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "axios": "1.7.5",
    "caniuse-lite": "1.0.30001662",
    "classnames": "2.5.1",
    "dayjs": "1.11.13",
    "next": "15.1.4",
    "next-auth": "4.24.7",
    "next-sanity": "8.5.0",
    "next-sanity-image": "6.1.1",
    "nodemailer": "6.9.14",
    "normalize.css": "8.0.1",
    "prisma": "5.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "5.3.0",
    "react-player": "2.16.0",
    "react-twitter-embed": "4.0.4",
    "request-ip": "3.3.0",
    "sass": "1.79.3",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.5",
    "@eslint/js": "9.18.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "eslint": "9.18.0",
    "eslint-config-next": "15.1.4",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.3",
    "globals": "15.9.0",
    "husky": "9.1.6",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "vitest": "2.1.1"
  },
  "packageManager": "pnpm@9.13.2",
  "engines": {
    "node": ">=18.20.0"
  }
}
